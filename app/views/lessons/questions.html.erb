<h2>問題一覧</h2>
<table border="5">
        <tr>
                <th>問題ID</th>
                <th>問題名</th>
                <th>結果</th>
                <th>開始時間</th>
                <th>終了時間</th>
                <th>解答</th>
                <th>履歴</th>
        </tr>
        <% @questions.each do |question| %>
                <tr>
                        <td><%= question.id %></td>
                        <td><%= question.title %></td>
                        <td> - </td>
                        <td><%= question.start_time.strftime(" %Y /  %m / %d - %H:%M:%S ") %></td>
                        <td><%= question.end_time.strftime(" %Y /  %m / %d - %H:%M:%S ") %></td>
                        <td> - </td>
                        <td> - </td>
                </tr>
        <% end %>
</table>
